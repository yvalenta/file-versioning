<%- model_class = FileVersion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:file_upload_id) %>:</strong></dt>
  <dd><%= @file_version.file_upload_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_version) %>:</strong></dt>
  <dd><%= @file_version.file_version %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              file_versions_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_file_version_path(@file_version), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              file_version_path(@file_version),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
